{
    "extends": ["airbnb-base", "prettier"],
    "plugins": [
        "prettier",
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "rules": {
        "prettier/prettier": "error",
        "indent": 2,
        "no-console": 0,
        "semi": [2, "never"],
        "import/prefer-default-export": "off",
        "no-underscore-dangle": 0
    }
}
